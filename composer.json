{
    "name": "jblaak/php-modules",
    "type": "library",
    "authors": [
        {
            "name": "Joris Blaak",
            "email": "joris@movelab.studio"
        }
    ],
    "bin": ["./modules"],
    "require": {
        "php": "^8.0",
        "nikic/php-parser": "^4.13",
        "graphp/graphviz": "^0.2.2",
        "phpstan/phpdoc-parser": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/var-dumper": "^6.0",
        "phpstan/phpstan": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "PhpModules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
